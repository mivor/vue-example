{
  "name": "vue-example-api",
  "version": "1.0.0",
  "description": "Web API for the vue-example app",
  "main": "main.js",
  "scripts": {
    "dev": "nodemon --exec npm run start",
    "start": "npm run lint && babel-node src/main.js || true",
    "build": "babel src -d dist",
    "serve": "node dist/main.js",
    "lint": "eslint -f node_modules/eslint-friendly-formatter ./src || true"
  },
